  "scripts": {
    "init:dev": "npm-run-all dev:prepare development",
    "dev:prepare": "universal-webpack --settings ./webpack/universal-webpack-settings.json prepare",
    "development": "NODE_ENV=development npm-run-all --parallel dev:client:build dev:server:build start:dev",
    "dev:server": "NODE_ENV=development WEBPACK_DLLS=1 nodemon ./bin/server.js --watch ./server --watch ./build/server --no-update-notifier",
    "dev:api": "NODE_ENV=development nodemon ./bin/api.js --watch ./api --no-update-notifier",
    "dev:server:build": "NODE_ENV=development WEBPACK_DLLS=1 webpack --mode development --config ./webpack/webpack.config.server.development.babel.js --watch --colors --display-error-details",
    "dev:client:build": "NODE_ENV=development webpack-serve --hot-client --require babel-register --config webpack/webpack.config.client.development.babel.js",
    "start:dev": "concurrently \"yarn dev:server\" \"yarn dev:api\"",
    "start:prod": "concurrently \"yarn prod:server\" \"yarn prod:api\"",
    "prod:server": "NODE_ENV=production node ./bin/server.js",
    "prod:api": "NODE_ENV=production node ./bin/api.js",
    "prod:build": "NODE_ENV=production webpack --mode production --colors --display-error-details --config ./webpack/webpack.config.server.production.babel.js",
    "dlls": "webpack --colors --display-error-details --config webpack/vendor.config.js",
    "build": "NODE_ENV=production webpack --mode production --colors --display-error-details --config ./webpack/webpack.config.client.production.babel.js"
  },
