{
  "bootstrapVersion": 4,
  "preBootstrapCustomizations": "./client/assets/scss/bootstrap/preBootstrapCustomizations.scss",
  "bootstrapCustomizations": "./client/assets/scss/bootstrap/bootstrapCustomizations.scss",
  "appStyles": "./client/assets/scss/app/theme.scss",
  "loglevel": "disabled",
  "env": {
    "development": {
      "styleLoaders": [
        "style-loader",
        "css-loader",
        "sass-loader",
