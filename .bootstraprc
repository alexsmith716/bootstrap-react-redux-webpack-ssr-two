    "production": {
      "styleLoaders": [
        "css-loader",
        "sass-loader",
      ],
      "extractStyles": true,
    }
  },
  "styles": {
    "alert": true,
    "badge": true,
    "breadcrumb": true,
    "button-group": true,
    "buttons": true,
    "card": true,
    "carousel": true,
    "close": true,
    "code": true,
    "custom-forms": true,
    "dropdown": true,
    "forms": true,
    "functions": true,
    "grid": true,
    "images": true,
    "input-group": true,
    "jumbotron": true,
    "list-group": true,
    "media": true,
    "mixins": true,
    "modal": true,
    "nav": true,
    "navbar": true,
    "pagination": true,
    "popover": true,
    "print": true,
    "progress": true,
    "reboot": true,
    "root": true,
    "tables": true,
    "tooltip": true,
    "transitions": true,
    "type": true,
    "utilities": true
  },
  "scripts": {
    "alert": true,
    "button": true,
    "carousel": true,
    "collapse": true,
    "dropdown": true,
    "modal": true,
